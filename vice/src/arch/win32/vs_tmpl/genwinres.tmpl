# name of the project
PROJECTNAME = genwinres

# project type: library, console, gui
PROJECTTYPE = console

# library files for application, one per line, preceeded by a tab
PROJECTLIBS =
	advapi32.lib
	comdlg32.lib
	gdi32.lib
	kernel32.lib
	odbc32.lib
	odbccp32.lib
	ole32.lib
	oleaut32.lib
