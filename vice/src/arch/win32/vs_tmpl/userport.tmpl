# name of the project
PROJECTNAME = userport

# project type: library, console, gui
PROJECTTYPE = library

# project dependencies, one per line, preceeded by a tab
PROJECTDEPS =
	base

# include directories, relative to the top src dir, one per line, preceeded by a tab
INCLUDEDIRS =
	c64\cart
	diag
	joyport
	rtc
	samplerdrv

# source files for normal compile, relative to the top src dir, one per line, preceeded by a tab
SOURCES =
	userport\userport.c
	userport\userport_4bit_sampler.c
	userport\userport_8bss.c
	userport\userport_dac.c
	userport\userport_diag_586220_harness.c
	userport\userport_digimax.c
	userport\userport_joystick.c
	userport\userport_rtc_58321a.c
	userport\userport_rtc_ds1307.c