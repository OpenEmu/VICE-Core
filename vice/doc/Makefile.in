	$(srcdir)/fixdox.sh txt <vicetmp.txt | LANG=C sed 's/ \+$$//' >$(builddir)/vice.txt
	rm -f vicetmp.txt

$(builddir)/vice.pdf:	$(srcdir)/vice.texi
@BUILD_PDF_TRUE@	LANG=C sed 's/@heading NO WARRANTY/@center NO WARRANTY/g' <$(srcdir)/vice.texi >vicepdf.texi
@BUILD_PDF_TRUE@	LANG=C $(TEXI2DVI) -q --clean --pdf "--texinfo=@set $(PLATFORM_DOX_FLAGS) " -o $(builddir)/vice.pdf vicepdf.texi
@BUILD_PDF_TRUE@		rm -f vicepdf.texi
@BUILD_PDF_FALSE@	touch $(builddir)/vice.pdf

$(builddir)/vice.guide:	$(srcdir)/vice.texi
@BUILD_AMIGAGUIDE_TRUE@	cp $(srcdir)/vice.texi tmp.texi
@BUILD_AMIGAGUIDE_TRUE@	$(srcdir)/fixdox.sh guide
@BUILD_AMIGAGUIDE_TRUE@	$(srcdir)/texi2guide.sh $(MAKEGUIDE) $(builddir)/vice.guide vicetmp.texi $(host_cpu) $(host_os)
@BUILD_AMIGAGUIDE_TRUE@	rm -f tmp.texi vicetmp.texi
@BUILD_AMIGAGUIDE_FALSE@	touch $(srcdir)/vice.guide

$(builddir)/vice.hlp:	$(srcdir)/vice.texi
@BUILD_HLP_TRUE@	cp $(srcdir)/vice.texi tmp.texi
@BUILD_HLP_TRUE@	$(srcdir)/fixdox.sh hlp
@BUILD_HLP_TRUE@	$(MAKERTF) -Dplatformwindows --hpj=vice.hpj --output=vice.rtf vicetmp.texi
@BUILD_HLP_TRUE@	$(HCRTF) -o VICE.HLP -xn vice.hpj
@BUILD_HLP_TRUE@	rm -f vice.hpj vice.rtf vicetmp.texi
@BUILD_HLP_TRUE@	mv VICE.HLP $(builddir)/vice.hlp
@BUILD_HLP_FALSE@	touch $(builddir)/vice.hlp

$(builddir)/vice.chm:	$(srcdir)/vice.texi
@BUILD_CHM_TRUE@	$(srcdir)/texi2chm.sh $(HHC) $(builddir)/vice.chm $(srcdir)/vice.texi $(srcdir)/fixdox.sh $(srcdir)/t2h.pl
@BUILD_CHM_FALSE@	touch $(builddir)/vice.chm

$(builddir)/vice.inf:	$(srcdir)/vice.texi
@BUILD_IPF_TRUE@	cp $(srcdir)/vice.texi tmp.texi
@BUILD_IPF_TRUE@	$(srcdir)/fixdox.sh ipf
@BUILD_IPF_TRUE@	$(TEXI2IPF) $(srcdir)/vicetmp.texi >temp.ipf
@BUILD_IPF_TRUE@	$(IPFC) -i temp.ipf
@BUILD_IPF_TRUE@	mv temp.inf $(builddir)/vice.inf
@BUILD_IPF_TRUE@	rm -f temp.ipf vicetmp.texi
@BUILD_IPF_FALSE@	touch $(builddir)/vice.inf

$(builddir)/vice.info:	$(srcdir)/vice.texi
	touch $(builddir)/vice.info
	$(MAKEINFO) -D$(PLATFORM_DOX_FLAGS) -o $(builddir)/vice.info --no-validate --no-split $(srcdir)/vice.texi

